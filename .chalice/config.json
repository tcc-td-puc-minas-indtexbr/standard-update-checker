          "lambda_timeout": 120
        }
      },
      "environment_variables": {
        "APP_ENV": "production",
        "DEBUG": "false",
        "LOG_LEVEL": "info",
        "API_SERVER": "https://services.hagatus.com.br/sigo-standard-update-checker",
        "API_SERVER_DESCRIPTION": "Production server",
        "DYNAMODB_TABLE_NAME": "standard-update-checker-dynamodb-StandardUpdatesTable-2QG9HALELHWY"
